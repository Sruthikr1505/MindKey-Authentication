{
  "version": 2,
  "builds": [
    {
      "src": "frontend/eeg-auth-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "env": {
          "NODE_VERSION": "20.x"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://eeg-auth-backend.onrender.com"
  }
}